{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext"],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "dist",
    "baseUrl": ".",
    "sourceMap": true,
    "paths": {
      "capabilities/*": [ "catalog/capabilities/*" ],
      "generators/*": [ "catalog/generators/*" ],
      "core/*": [ "lib/core/*" ]
    }
  },
  "exclude": [
    "catalog/generators/*/files"
  ]
}
